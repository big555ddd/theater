<%- include("../partials/header.ejs") %>
<div class="container">
<h1>Add your comment</h1>
<form action="/prints/<%= print._id %>/comments" method="POST">
<div class="mb-3">
    <label class="form-label">Comment</label>
    <input type="text" class="form-control" name="comment[text]" placeholder="Your comment">
</div>
<button type="submit" class="btn btn-primary">Submit</button>
<a href="/prints/<%= print._id %>">Go back</a>
</form>
</div>

<%- include("../partials/footer.ejs") %>